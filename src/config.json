{
  "threadsCount": 1,
  "cache": {
    "type": "redis",
    "host": "127.0.0.1",
    "port": 6378,
    "defaultTimeout": 300
  },
  "instances": [
    {
      "tag": "test001",
      "cacheFrequency": 30,
      "syncFrequency": 0,
      "httpConfig": {
        "hostUrl": "http://localhost:8082",
        "header": {
          "header1": "test1",
          "header2": "test2"
        },
        "username": null,
        "password": null
      },
      "database": {
        "type": "postgres",
        "hostOrPath": "127.0.0.1",
        "port": 5432,
        "charset": "utf8",
        "dbname": "testDatabase",
        "username": "root",
        "password": "",
      },
      "syncs": [
        {
          "endpoint": {
            "path": "/receive?id={{id}}&name={{name}}",
            "type": "POST",
            "ValidStatusCode": 200,
            "ValidStringRegex": "Ok",
            "extraHeader": {
              "header1": "test1",
              "header2": "test2"
            },
            "body": {
              "var1": "test1",
              "var2": "test1",
              "test_id": "{{id}}",
              "test_name": "{{name}}"
            }
          },
          "query": {
            "check": "select count(1) from tests where sync = 0; /* checks counts of pending sync */",
            "pick": "select id, name from tests where sync = 0; /* selects list of pending sync */",
            "confirm": "update tests set sync = 1 where id in {{id}}; /* Set a particular field to confirm syncs, with list of field picked from result */"
          }
        }
      ]
    },
    {
      "info": "Second row.."
    }
  ]
}